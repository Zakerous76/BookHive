{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "src/**/*", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
